{
  "$schema": "https://ev2schema.azure.net/schemas/2020-01-01/rolloutSpecification.json",
  "rolloutMetadata": {
    "serviceModelPath": "USNat\\ServiceModel.json",
    "scopeBindingsPath": "USNat\\ScopeBindings.json",
    "name": "Applens Staging",
    "rolloutType": "Major",
    "buildSource": {
      "parameters": {
        "versionFile": "buildver.txt"
      }
    },
    "notification": {
      "email": {
        "to": "applensdeployments@microsoft.com",
        "options": {
          "when": [
            "onError",
            "onComplete"
          ],
          "verbosity": "Compact"
        }
      }
    }
  },
  "orchestratedsteps": [
    {
      "name": "AppSettingsUpdate",
      "targetType": "ServiceResource",
      "targetName": "AppSettingsUpdate",
      "actions": [
        "Deploy"
      ]
    },
    {
      "name": "ZipDeploy",
      "targetType": "ServiceResource",
      "targetName": "ApplensZipDeploy",
      "actions": [
        "Deploy"
      ],
      "dependsOn": [
        "AppSettingsUpdate"
      ]
    }
  ]
}