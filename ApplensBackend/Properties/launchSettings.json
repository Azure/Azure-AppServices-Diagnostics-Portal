{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:5000/",
      "sslPort": 0
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "AppLensV3": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "CloudDomain": "PublicAzure"
      }
    },
    "AppLensV3Fairfax": {
      "commandName": "Project",
      "commandLineArgs": "--useHttps=true",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "CloudDomain": "Fairfax"
      }
    },
    "AppLensV3NationalCloudDevelopment": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "NationalCloudDevelopment"
      }
    }
  }
}